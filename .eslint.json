{
  "env": {
    "browser": true
  },
  "globals": {
    "_": false,
    "Backbone": false,
    "jQuery": false,
    "JSON": false,
    "wp": false,
    "StripeCheckout": false,
    "spShared": false,
    "accounting": false
  },
  "rules": {
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "curly": 2,
    "eqeqeq": 2,
    "no-eq-null": 2,
    "no-unused-expressions": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-caller": 2,
    "no-irregular-whitespace": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "dot-notation": [
      2,
      {
        "allowKeywords": true,
        "allowPattern": "^[a-z]+(_[a-z]+)+$"
      }
    ],
    "space-unary-ops": [
      2,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "vars-on-top": 2,
    "yoda": [
      2,
      "always"
    ],
    "camelcase": [
      2,
      {
        "properties": "never"
      }
    ]
  }
}
